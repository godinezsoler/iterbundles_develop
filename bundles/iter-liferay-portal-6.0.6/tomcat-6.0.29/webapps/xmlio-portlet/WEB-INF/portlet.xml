<?xml version="1.0" encoding="UTF-8"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd">
	<portlet>
		<portlet-name>xmlio-portlet</portlet-name>
		<display-name>Xmlio</display-name>
		<portlet-class>com.protecmedia.iter.xmlio.portlet.XmlIOPortlet</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/xmlio-portlet/view.jsp</value>
		</init-param>
		<init-param>
			<name>edit-channel-jsp</name>
			<value>/html/xmlio-portlet/editChannel.jsp</value>
		</init-param>
		<init-param>
			<name>live-configuration-jsp</name>
			<value>/html/xmlio-portlet/viewLiveConfiguration.jsp</value>
		</init-param>
		<init-param>
			<name>live-control-jsp</name>
			<value>/html/xmlio-portlet/viewLiveControl.jsp</value>
		</init-param>
		<init-param>
			<name>live-item-jsp</name>
			<value>/html/xmlio-portlet/viewLiveDetails.jsp</value>
		</init-param>
		<init-param>
			<name>channel-control-jsp</name>
			<value>/html/xmlio-portlet/viewChannelControl.jsp</value>
		</init-param>
		<init-param>
			<name>channel-control-log-jsp</name>
			<value>/html/xmlio-portlet/viewChannelControlLog.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>Xmlio</title>
			<short-title>Xmlio</short-title>
			<keywords>Xmlio</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>publish-to-live-portlet</portlet-name>
		<display-name>PublishToLive</display-name>
		<portlet-class>com.protecmedia.iter.xmlio.portlet.PublishToLivePortlet</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/publish-to-live-portlet/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>PublishToLive</title>
			<short-title>PublishToLive</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>xmlio-cronjob-portlet</portlet-name>
		<display-name>XMLIO CronJob</display-name>
		<portlet-class>com.liferay.util.bridges.mvc.MVCPortlet</portlet-class>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>XMLIO CronJob</title>
			<short-title>XMLIO CronJob</short-title>
			<keywords>XMLIO CronJob</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>